    "@prisma/generator-helper": "^6.1.0",
    "@types/node": "^22.10.3",
    "changesets": "^1.0.2",
    "prettier": "^3.4.2",
    "prisma": "^6.1.0",
    "typescript": "^5.7.2",
    "tsup": "^8.3.5"
  },
  "keywords": [
    "prisma",
    "typescript",
    "prisma-enums",
    "enums",
    "enum",
    "graphql"

{
  "name": "prisma-enums-generator",
  "version": "1.0.0",
  "description": "Prisma enum generator",
  "main": "dist/bin.js",
  "types": "./dist/bin.d.ts",
  "module": "./dist/bin.mjs",
  "private": false,
  "files": [
    "dist"
  ],
  "bin": {
    "prisma-enums-generator": "dist/bin.js"
  },

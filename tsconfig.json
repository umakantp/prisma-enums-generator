{
  "compilerOptions": {
    "target": "ES2022" ,
    "module": "commonjs" ,
    "esModuleInterop": true ,
    "forceConsistentCasingInFileNames": true ,
    "outDir": "./dist",
    "rootDir": "./src",
    "strict": true ,
    "skipLibCheck": true,
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules", "dist", "prisma"]
}
